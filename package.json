{
  "name": "contentful-metalsmith-example",
  "version": "1.0.0",
  "description": "A example app that uses the contentful-metalsmith plugin",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/contentful-labs/contentful-metalsmith-example"
  },
  "scripts": {
    "dev": "run-p watch serve",
    "serve": "live-server ./build",
    "start": "node build.js",
    "lint": "standard \"helpers/**/*.js\" \"build.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "gazer --pattern '{src,layouts,helpers,partials}/**/*' node build.js"
  },
  "license": "MIT",
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "contentful-management": "^5.1.3",
    "contentful-metalsmith": "^0.11.0",
    "dotenv": "^6.0.0",
    "gazer": "^1.0.0",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "jstransformer-handlebars": "^1.1.0",
    "jstransformer-nunjucks": "^0.5.0",
    "metalsmith": "^2.3.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-clean": "0.0.0",
    "metalsmith-data-markdown": "0.0.3",
    "metalsmith-debug-ui": "^0.3.0",
    "metalsmith-discover-helpers": "^0.1.0",
    "metalsmith-discover-partials": "^0.1.0",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-env": "^2.1.1",
    "metalsmith-in-place": "^4.1.1",
    "metalsmith-layouts": "^2.1.0",
    "metalsmith-markdown": "^0.2.2",
    "metalsmith-sass": "^1.5.1"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0",
    "live-server": "*",
    "npm-run-all": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
